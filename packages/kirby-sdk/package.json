{
  "name": "@kirby/sdk",
  "version": "0.1.0",
  "description": "The official Kirby SDK for JavaScript",
  "keywords": [
    "kirby",
    "kirby-sdk",
    "performance",
    "metrics"
  ],
  "sideEffects": false,
  "cjs": "lib/index.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/kirby.js",
  "unpkg": "dist/kirby.min.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "clean": "rimraf ./dist ./lib ./es ./.jest",
    "build": "npm run clean && npm-run-all build:es build:cjs build:umd build:umd:min",
    "build:es": "tsc -m es2015 -t es5 --outDir es",
    "build:cjs": "tsc -m commonjs -t es5 --outDir lib",
    "build:umd": "cross-env rollup -c scripts/rollup.config.js",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=commonjs rollup -c scripts/rollup.config.js && gzip dist/kirby-sdk.min.js -c > dist/kirby-sdk.min.js.gz",
    "prettier": "prettier -c --no-editorconfig --write '**/*'",
    "prettier-quick": "pretty-quick",
    "test": "jest --no-cache"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "^26.0.9",
    "@types/requestidlecallback": "^0.3.1",
    "babel-jest": "^26.2.2",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jsdom": "^16.3.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.1.4",
    "tslib": "^2.0.0",
    "typescript": "^3.8.3",
    "typings": "^2.1.1",
    "uglify-es": "^3.3.9",
    "usertiming": "^0.1.8",
    "version-changelog": "^3.1.1"
  },
  "dependencies": {
    "fingerprintjs2": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run prettier",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsejx/kirby.git"
  },
  "author": "tsejx <tsejx@foxmail.com>",
  "license": "MIT"
}
