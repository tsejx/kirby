{
  "name": "@kirby/sdk",
  "version": "0.1.0",
  "description": "The official Kirby SDK for JavaScript",
  "keywords": [
    "kirby",
    "kirby-sdk",
    "performance",
    "metrics"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/kirby.min.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:commonjs": "tsc -m CommonJS -t ES3 --outDir lib",
    "build:es": "tsc -m ES6 -t ES3 --outDir es",
    "build:umd": "cross-env rollup -c scripts/rollup.config.js",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=commonjs rollup -c scripts/rollup.config.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf ./dist ./lib ./es",
    "prettier": "prettier -c --write '**/*'",
    "prettier-quick": "pretty-quick"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.23.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.0.0",
    "typescript": "^3.8.3",
    "uglify-es": "^3.3.9"
  },
  "dependencies": {},
  "directories": {
    "example": "example"
  },
  "author": "tsejx <tsejx@foxmail.com>",
  "license": "MIT"
}
